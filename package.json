{
  "name": "family_app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cy:open": "cypress open --config-file cypress.config.cjs",
    "cy:run": "cypress run --config-file cypress.config.cjs",
    "test:e2e": "start-server-and-test dev http://localhost:5173 \"cypress run --config-file cypress.config.cjs\"",
    "test-open:e2e": "start-server-and-test dev http://localhost:5173 \"cypress open --config-file cypress.config.cjs\"",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ci": "vitest run --coverage --reporter=dot",
    "coverage:e2e": "start-server-and-test dev http://localhost:5173 \"cypress run --config-file cypress.config.cjs\" && nyc report --reporter=lcov --report-dir coverage-e2e",
    "coverage:merge": "node scripts/merge-lcov.mjs coverage-unit/lcov.info coverage-e2e/lcov.info coverage/lcov.info",
    "coverage:all": "npm run test:unit:coverage && npm run coverage:e2e && npm run coverage:merge"
  },
  "dependencies": {
    "@apollo/client": "^4.0.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-charts": "^8.9.0",
    "@mui/x-data-grid": "^8.9.1",
    "@testing-library/dom": "^10.4.1",
    "graphql": "^16.11.0",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.13.0",
    "@eslint/js": "^9.30.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
  "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.17.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^24.1.3",
    "nyc": "^15.1.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "start-server-and-test": "^2.0.6",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.1.7",
    "vite-plugin-istanbul": "^5.0.0"
  }
}
